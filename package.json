{
    "name": "ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "check": "npm run type-check && npm run prettier && npm run lint",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --write .",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "highlight.js": "^11.11.1",
        "tailwindcss": "^4.1.11",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@eslint/css": "^0.9.0",
        "@eslint/js": "^9.30.1",
        "@eslint/json": "^0.12.0",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.30.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-vue": "^10.4.0",
        "gh-pages": "^6.3.0",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.13",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^6.3.5",
        "vue-tsc": "^2.2.8"
    }
}
